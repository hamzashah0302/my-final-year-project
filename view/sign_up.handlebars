    {{!-- ====================================> --}}

    <div class="limiter">
		<div class="container-login100" style="background-image: url('/view/images/img-01.jpg');">
			<div class="wrap-login100 p-t-190 p-b-30" >
				
				<form class="login100-form validate-form" action="/sign_up" method="POST">
					<div class="login100-form-avatar" style="margin-bottom: 10px">
						<img src="/view/images/logo.jpg" alt="AVATAR">
					</div>

					<span class="login100-form-title p-t-20 p-b-45" style="margin-bottom: 30px">
						SignUp Here <br><br>
						please choose your username and Password
					</span>

					<div class="wrap-input100 validate-input m-b-10" data-validate = "Username is required" style="margin-bottom: 10px">
						
						<input class="input100" type="text" name="new_user" placeholder="User Name">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<i class="fa fa-user"></i>
						</span>
					</div>

					<div class="wrap-input100 validate-input m-b-10" data-validate = "Password is required" style="margin-bottom: 10px">
						<input class="input100" type="password" name="pass" placeholder="Password">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<i class="fa fa-lock"></i>
						</span>
					</div>
					<div class="container-login100-form-btn p-t-10">
						
						<button type="submit" class="login100-form-btn">
							Sign Up
						</button>
					</div>
				</form>
			</div>
		</div>
	</div>
	
	<script src="/view/vendor/jquery/jquery-3.2.1.min.js"></script>
	<script src="/view/vendor/bootstrap/js/popper.js"></script>
	<script src="/view/vendor/bootstrap/js/bootstrap.min.js"></script>
	<script src="/view/vendor/select2/select2.min.js"></script>
	<script src="/view/js/main.js"></script>